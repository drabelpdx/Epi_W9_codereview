<div class="row-fluid">
 <div class="col-md-4">
   <table class='table'>
     <thead>
       <tr><th>Questions</th></tr>
     </thead>

     <tbody>
       {{#each question in model}}
         <tr>
           <td>
             {{question.asked}} <small class='muted'>  by:  {{question.author}}</small>
           </td>
         </tr>
       {{/each}}
     </tbody>
   </table>
 </div>

 <div class="col-md-8">

   {{outlet}}

 </div>
</div>

